   "source": [
    "#Using 2 yield statements, create a generator which alternatively yields 'yes' and 'no'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "yes\n",
      "no\n"
     ]
    }
   ],
   "source": [
    "def yes_no():\n",
    "    print('yes')\n",
    "    yield\n",
    "    print('no')\n",
    "    yield\n",
    "yn= yes_no()\n",
    "next(yn)\n",
    "next(yn)"
   ]
  }
